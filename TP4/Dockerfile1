FROM debian:11
RUN apt-get update 
RUN apt-get install build-essential, python-dev, python3-pip, default-libmysqlclient-dev
RUN pip3 install -upgrade pip
RUN pip install flask-mysqldb, mysql-connector-python
CMD ["mkdir", "/srv/templates"]
CMD ["cp", "index.html", "/srv/templates"]
CMD ["cp", "app1.py", "/srv/"]